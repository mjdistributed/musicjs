{
   "name" : "Muslogger Extension",
   "description" : "Log songs from Google Play on Muslogger",
   "version" : "0",
   "manifest_version": 2,
   "permissions" : [
      "tabs",
      "http://play.google.com/",
      "https://play.google.com/",
      "http://muslogger.appspot.com/log",
      "http://muslogger.appspot.com/login",
      "https://www.google.com/accounts/ServiceLogin*",
      "https://accounts.google.com/ServiceLogin*",
      "https://appengine.google.com/_ah/conflogin?continue=http%3A%2F%2Fmuslogger*"
   ],
   "background": {
    "scripts": ["js/settings.js", "util.js", "background.js"],
    "persistent": false
   },
   "icons" : {
      "48" : "img/footprint48.png"
   },
   "browser_action" : {
      "default_icon" : "img/footprint48.png"
   },
   "content_scripts" : [
      {
         "matches" : [
            "*://play.google.com/music/listen*"
         ],
         "js" : [
            "js/inject.js",
            "js/settings.js",
            "js/contentscript.js"
         ]
      },
      {
         "run_at" : "document_start",
         "matches" : [
            "*://play.google.com/music/listen*"
         ],
         "js" : [
            "js/jquery-1.7.min.js"
         ]
      }
   ],
    "commands": {
      "toggle_play": {
        "suggested_key": {
          "default": "MediaPlayPause",
          "linux": "Ctrl+Shift+8"
        },
        "description": "Toggle play/pause",
        "global": true
      },
      "prev_song": {
        "suggested_key": {
          "default": "MediaPrevTrack",
          "linux": "Ctrl+Shift+7"
        },
        "description": "Previous song",
        "global": true
      },
      "next_song": {
        "suggested_key": {
          "default": "MediaNextTrack",
          "linux": "Ctrl+Shift+9"
        },
        "description": "Next song",
        "global": true
      },
      "goto_play_tab": {
        "suggested_key": {
          "default": "Ctrl+Shift+0"
        },
        "description": "Go to Google Play tab",
        "global": true
      }
    }
}